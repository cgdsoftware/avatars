{
    "name": "laravel-enso/avatars",
    "description": "User Avatar manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "laravel",
        "avatars",
        "avatar"
    ],
    "homepage": "https://github.com/laravel-enso/avatars",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-enso/core": "4.8.*",
        "laravel-enso/files": "3.4.*",
        "laravel-enso/image-transformer": "1.6.*",
        "laravel-enso/migrator": "1.1.*",
        "laravolt/avatar": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Avatars\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Avatars\\AppServiceProvider",
                "LaravelEnso\\Avatars\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}