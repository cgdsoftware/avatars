{
    "name": "laravel-enso/avatarmanager",
    "description": "User Avatar manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "avatarmanager",
        "avatar-manager",
        "avatar"
    ],
    "homepage": "https://github.com/laravel-enso/AvatarManager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravolt/avatar": "2.0.*",
        "laravel-enso/core": "4.2.*",
        "laravel-enso/filemanager": "3.0.*",
        "laravel-enso/structuremanager": "3.0.*",
        "laravel-enso/imagetransformer": "1.4.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\AvatarManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\AvatarManager\\AppServiceProvider",
                "LaravelEnso\\AvatarManager\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
